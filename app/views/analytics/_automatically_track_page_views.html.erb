<script defer async>
if (typeof define === "function" && define.amd) {
  require(['analytics'], function(analytics){
    analytics.setJTI('<%= @_js_tracking_id %>')
    analytics.automaticallyTrackPageView()
  })
} else {
  analytics.setJTI('<%= @_js_tracking_id %>') 
  analytics.automaticallyTrackPageView()
}
</script>